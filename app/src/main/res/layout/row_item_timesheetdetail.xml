<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <androidx.cardview.widget.CardView
            android:id="@+id/parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="8dp"
            android:elevation="@dimen/margin_2x"
            app:cardCornerRadius="@dimen/margin_2x"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mTvTimeSheetDate">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/mTvJobTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/normal_poppins"
                    android:text="Date "
                    android:textColor="@color/leads_color"
                    android:textSize="@dimen/text_very_small"
                    app:layout_constraintEnd_toStartOf="@+id/textView119"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    android:background="@drawable/button_rounded_shape_new"
                    android:backgroundTint="@color/colorPrimary"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/mTvJobTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginTop="2dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginRight="4dp"
                        android:layout_marginBottom="2dp"
                        android:fontFamily="@font/normal_poppins"
                        android:gravity="center"
                        android:text="12 Hours"
                        android:textAllCaps="false"
                        android:textColor="@color/white_color"
                        android:textSize="@dimen/text_small"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>


                <TextView
                    android:id="@+id/mTvInAddress"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:fontFamily="@font/normal_poppins"
                    android:gravity="center_vertical"
                    android:padding="@dimen/margin_1_1x"
                    android:text="32 Rock Street, NY Street"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/text_small"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/mTvInAddress9"
                    app:layout_constraintStart_toStartOf="@+id/mTvInAddress9"
                    app:layout_constraintTop_toBottomOf="@+id/mTvInAddress9" />

                <TextView
                    android:id="@+id/mTvInAddress9"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/normal_poppins"
                    android:padding="@dimen/margin_1_1x"
                    android:text="Clock In Address"
                    android:textColor="@color/leads_color"
                    android:textSize="@dimen/text_very_small"
                    app:layout_constraintEnd_toStartOf="@+id/textView118"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="@+id/mTvJobTitle"
                    app:layout_constraintTop_toBottomOf="@+id/mTvDate" />

                <TextView
                    android:id="@+id/mTvDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/normal_poppins"
                    android:text="Dec 12, 2021"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/text_small"
                    app:layout_constraintEnd_toEndOf="@+id/mTvJobTitle"
                    app:layout_constraintStart_toStartOf="@+id/mTvJobTitle"
                    app:layout_constraintTop_toBottomOf="@+id/mTvJobTitle" />

                <TextView
                    android:id="@+id/textView119"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"
                    android:fontFamily="@font/normal_poppins"
                    android:text="In Time "
                    android:textColor="@color/leads_color"
                    android:textSize="@dimen/text_very_small"
                    app:layout_constraintEnd_toStartOf="@+id/textView120"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/mTvJobTitle"
                    app:layout_constraintTop_toTopOf="@+id/mTvJobTitle" />

                <TextView
                    android:id="@+id/textView120"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:fontFamily="@font/normal_poppins"
                    android:text="Out Time "
                    android:textColor="@color/leads_color"
                    android:textSize="@dimen/text_very_small"
                    app:layout_constraintEnd_toStartOf="@+id/textView7"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/textView119"
                    app:layout_constraintTop_toTopOf="@+id/textView119" />

                <TextView
                    android:id="@+id/mTvInTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/normal_poppins"
                    android:text="12:00 AM"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/text_small"
                    app:layout_constraintEnd_toEndOf="@+id/textView119"
                    app:layout_constraintStart_toStartOf="@+id/textView119"
                    app:layout_constraintTop_toBottomOf="@+id/textView119" />

                <TextView
                    android:id="@+id/mTvOutTime"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/normal_poppins"
                    android:text="06:30 PM"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/text_small"
                    app:layout_constraintEnd_toStartOf="@+id/textView7"
                    app:layout_constraintStart_toStartOf="@+id/textView120"
                    app:layout_constraintTop_toBottomOf="@+id/textView120" />

                <TextView
                    android:id="@+id/textView118"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/normal_poppins"
                    android:padding="@dimen/margin_1_1x"
                    android:text="Clock Out Address"
                    android:textColor="@color/leads_color"
                    android:textSize="@dimen/text_very_small"
                    app:layout_constraintEnd_toEndOf="@+id/textView7"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/mTvInAddress9"
                    app:layout_constraintTop_toTopOf="@+id/mTvInAddress9" />

                <TextView
                    android:id="@+id/mTvOutAddress"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="4dp"
                    android:padding="@dimen/margin_1_1x"
                    android:text="32 Rock Street, NY Street"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/text_small"
                    android:fontFamily="@font/normal_poppins"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/textView118"
                    app:layout_constraintStart_toStartOf="@+id/textView118"
                    app:layout_constraintTop_toBottomOf="@+id/textView118" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <com.makeramen.roundedimageview.RoundedImageView
                android:id="@+id/imageView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:src="@drawable/ic_noteslist"
                android:visibility="gone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/mTvTimeSheetDate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="6dp"
            android:fontFamily="@font/medium_poppins"
            android:text="Today, Sept 3"
            android:textColor="@color/leads_color"
            android:textSize="@dimen/text_mediumLarge"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>